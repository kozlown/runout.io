{
  "name": "runout.io",
  "version": "1.0.0",
  "description": "Web Multiplayer Platformer",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run buildFront && npm run buildBack",
    "buildFront": "cd ./front/react-app && npm run build",
    "buildBack": "webpack",
    "watchBack": "webpack --watch",
    "runBack": "sudo nodemon bin/bundle",
    "watchFront": "cd ./front/react-app && npm run start",
    "prod": "npm run build && pm2 start bin/bundle.js",
    "stop": "pm2 stop bundle && pm2 delete bundle"
  },
  "repository": {
    "type": "git",
